{
  "name": "waypoint-labs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true});\" && next dev",
    "dev:mobile": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true});\" && next dev -H 0.0.0.0",
    "build": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true});\" && next build",
    "start": "next start",
    "lint": "next lint",
    "ngrok": "node scripts/ngrok.js",
    "watermarks:split": "node scripts/split-watermarks.js public/watermarks/source/4way.png public/watermarks/source/control-center.png"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.47",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.0"
  }
}
